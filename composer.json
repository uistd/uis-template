{
  "name": "uistd/uis-template",
  "description": "FFan Dop UIService Basic Template. composer create-project uis/dop-uis-template uis_template dev-master",
  "keywords": [
    "ui service",
    "dop",
    "uis",
    "project template"
  ],
  "type": "project",
  "license": "BSD-3-Clause",
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6.0",
    "uistd/dop-uis": "3.*",
    "uistd/dop": "3.*",
    "uistd/php-dop": "3.*",
    "uistd/tpl": "2.*",
    "uistd/mysql": "2.*"
  },
  "autoload": {
    "psr-4": {
      "Protocol\\": "protocol",
      "Sdk\\": "sdk",
      "Dao\\" : "dao"
    }
  },
  "config": {
    "secure-http": false
  }
}
