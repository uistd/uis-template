{
  "name": "uistd/uis-template",
  "description": "FFan Dop UIService Basic Template. composer create-project uistd/uis-template uis_template dev-master",
  "keywords": [
    "ui service",
    "dop",
    "uis",
    "project template"
  ],
  "type": "project",
  "license": "BSD-3-Clause",
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6.0",
    "uistd/dop-uis": "3.*",
    "uistd/dop": "3.*",
    "uistd/php-dop": "3.*",
    "uistd/tpl": "2.*",
    "uistd/mysql": "2.*"
  },
  "autoload": {
    "psr-4": {
      "Protocol\\": "protocol",
      "Sdk\\": "sdk",
      "Dao\\" : "dao"
    }
  },
  "config": {
    "secure-http": false
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://composer.intra.ffan.com/repo/private/"
    },
    {
      "type": "composer",
      "url": "http://composer.intra.ffan.com/repo/packagist/"
    },
    {
      "packagist": false
    }
  ]
}
