{
  "name": "ffan/dop-uis-template",
  "description": "FFan Dop UIService Basic Template. composer create-project ffan/dop-uis-template uis_template dev-master",
  "keywords": [
    "ffan",
    "dop",
    "uis",
    "project template"
  ],
  "type": "project",
  "license": "BSD-3-Clause",
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6.0",
    "ffan/dop-uis": "2.*",
    "ffan/dop": "2.*",
    "ffan/php-dop": "2.*",
    "ffan/cluster-redis": "1.*",
    "ffan/std-tpl": "1.*",
    "ffan/std-mysql": "1.*",
    "ffan/std-pubsub": "1.*"
  },
  "autoload": {
    "psr-4": {
      "FFan\\Uis\\Base\\": "base",
      "Protocol\\": "protocol"
    }
  },
  "config": {
    "secure-http": false
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://composer.intra.ffan.com/repo/private/"
    },
    {
      "type": "composer",
      "url": "http://composer.intra.ffan.com/repo/packagist/"
    },
    {
      "packagist": false
    }
  ]
}
