{
  "name": "uistd/uis-template",
  "description": "FFan Dop UIService Basic Template. composer create-project uistd/uis-template uis_template dev-master",
  "keywords": [
    "ui service",
    "dop",
    "uis",
    "project template"
  ],
  "type": "project",
  "license": "BSD-3-Clause",
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6.0",
    "uistd/dop-uis": "3.*",
    "uistd/dop": "3.*",
    "uistd/php-dop": "3.*",
    "uistd/tpl": "2.*",
    "uistd/mysql": "2.*"
  },
  "autoload": {
    "psr-4": {
      "Uis\\Protocol\\": "protocol",
      "Uis\\Sdk\\": "sdk",
      "Uis\\Dao\\": "dao",
      "Uis\\Helper\\": "helper",
      "Uis\\Page\\": "page",
      "Uis\\Filter\\": "filter",
      "Uis\\Activity\\": "activity",
      "Uis\\Work\\": "work",
      "Uis\\App\\": "apps",
      "Uis\\Mock\\": "mock",
      "Uis\\Tool\\": "tool/class"
    }
  },
  "config": {
    "secure-http": false
  }
}
