<?xml version="1.0" encoding="UTF-8"?>
<protocol>
    <action name="index" note="简单的测试">
        <request>
            <int name="a" default="10">
                <plugin_valid range="1," range-msg="最小值1"/>
            </int>
            <int name="b" default="100">
                <plugin_valid range="1," range-msg="最小值1"/>
            </int>
        </request>
        <response>
            <model name="data">
                <int name="plus" note="加">
                    <plugin_mock range="1, 100"/>
                </int>
                <int name="minus" note="减">
                    <plugin_mock range="1, 10"/>
                </int>
                <int name="multiply" note="乘">
                    <plugin_mock range="100, 1000"/>
                </int>
                <float name="divide" note="除">
                    <plugin_mock range="0.1, 10"/>
                </float>
            </model>
        </response>
    </action>
</protocol>